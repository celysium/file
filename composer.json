                "Celysium\\File\\FileServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name" : "Nasser Hekmati",
            "email" : "hekmati.nasser@gmail.com"
        },
        {
            "name": "Ehsan Tavakoli",
            "email": "ehsantavakoli1995@gmail.com"
        }
    ],
    "require": {
        "php" : "^8.0",
        "laravel/framework" : "^9.0",
        "celysium/responser": "dev-master",
        "celysium/base-structure": "dev-master"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
