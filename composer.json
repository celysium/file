        "Laravel",
        "File",
        "Image"
    ],
    "license": "MIT",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Celysium\\File\\": "src/",
            "Celysium\\File\\Database\\Factories\\": "database/factories"
        }
